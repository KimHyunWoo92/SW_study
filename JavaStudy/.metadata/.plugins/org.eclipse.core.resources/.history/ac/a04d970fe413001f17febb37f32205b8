package ex04_반복문;

import java.util.Scanner;

public class ex06_약수구하기 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int num = sc.nextInt();
		
		for (int i=1; i<=num;,i++) {
			if (num%i ==0) {
				System.out.print(i+"는"+num+"의 약수입니다.");
			}
		}

	}

}
